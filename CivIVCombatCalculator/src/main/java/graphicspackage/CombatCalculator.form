    <Component class="javax.swing.JLabel" name="aStrengthTitle">
      <Properties>
        <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
          <Font name="Times New Roman" size="14" style="0"/>
        </Property>
        <Property name="text" type="java.lang.String" value="&lt;html&gt;Strength Value between 1 and 400&lt;br&gt;Remember, combat compares ratios.&lt;/html&gt;"/>
        <Property name="toolTipText" type="java.lang.String" value="&lt;html&gt;Combat in Civilization IV works by relative Strength between the parties.&lt;br&gt;&#xa;Therefore, Combat between Strengths 9 and 10, 18 and 20 and 360 and 400 are all the same&lt;br&gt;&#xa;for all practical purposes.&lt;br&gt;&#xa;Thus, if you want to give fractional Strength as input, just multiply the Strength of both parties by 10.&lt;/html&gt;"/>
        <Property name="horizontalTextPosition" type="int" value="0"/>
      </Properties>
    </Component>
    <Component class="javax.swing.JLabel" name="aBonusTitle">
      <Properties>
        <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
          <Font name="Times New Roman" size="14" style="0"/>
        </Property>
        <Property name="text" type="java.lang.String" value="&lt;html&gt;Combat Bonus percentage.&lt;br&gt; Value between 0 and 300&lt;/html&gt;"/>
        <Property name="toolTipText" type="java.lang.String" value="&lt;html&gt;Combat Bonus works by adding a percentage of the Strength of the party to the Strength of that Party.&lt;br&gt;&#xa;Unlike one would expect, opposing Bonus-percentages are directly subtracted from one another until&lt;br&gt;&#xa;only at most one party has Bonus left. &lt;br&gt;&#xa;Thus, if you want to give a party a negative &quot;Bonus&quot;, just add that amount to the Bonus of the other Party.&lt;/html&gt;"/>
      </Properties>
    </Component>
    <Component class="javax.swing.JSpinner" name="aBonus">
      <Properties>
        <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
          <Font name="Times New Roman" size="14" style="0"/>
        </Property>
        <Property name="model" type="javax.swing.SpinnerModel" editor="org.netbeans.modules.form.editors2.SpinnerModelEditor">
